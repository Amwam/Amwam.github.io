{
  "name": "spa-github-pages",
  "version": "2.0.0",
  "description": "Single Page Apps for GitHub Pages",
  "scripts": {
    "start": "parcel watch src/index.html",
    "build": "parcel build src/index.html --out-dir ./",
    "eslint": "eslint **/*.jsx",
    "prettier": "prettier --write \"components/**/*.{js,jsx}\"",
    "precommit": "yarn prettier && yarn build"
  },
