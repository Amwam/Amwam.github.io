  "license": "MIT",
  "dependencies": {
    "babel-preset-env": "^1.6.1",
    "current-input": "^1.1.1",
    "git-pre-commit": "^2.1.3",
    "parcel": "^1.10.3",
    "prettier": "^1.11.1",
    "prismjs": "^1.13.0",
    "radium": "^0.24.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-markdown": "^2.5.0",
    "react-router": "^3.0.2",
    "react-sidebar": "^2.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-react": "^6.24.1",
