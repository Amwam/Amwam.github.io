{
  "name": "spa-github-pages",
  "version": "2.0.0",
  "description": "Single Page Apps for GitHub Pages",
  "scripts": {
    "start": "parcel src/index.html --no-cache",
    "build": "parcel build src/index.html --out-dir ./",
    "eslint": "eslint **/*.jsx",
    "prettier": "prettier --write \"{components,src}/**/*.{ts,tsx,js,jsx}\"",
    "precommit": "yarn prettier && python cleanup.py && yarn build && git add src.* index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rafrex/spa-github-pages.git"
  },
  "author": "Amit Shah <amit@amwam.me>",
  "license": "MIT",
  "dependencies": {
    "@types/react": "^16.7.3",
    "@types/react-dom": "^16.0.9",
    "@types/react-sidebar": "^3.0.0",
    "babel-preset-env": "^1.6.1",
    "current-input": "^1.1.1",
    "git-pre-commit": "^2.1.3",
    "parcel": "^1.10.3",
    "prettier": "^1.11.1",
    "prismjs": "^1.13.0",
    "radium": "^0.24.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-markdown": "^2.5.0",
    "react-router": "^3.0.2",
    "react-sidebar": "^2.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "live-server": "^1.2.0",
    "redbox-react": "^1.5.0",
    "typescript": "^3.1.6"
  }
}
