{
  "name": "amwam-website",
  "version": "2.0.0",
  "description": "Single Page Apps for GitHub Pages",
  "scripts": {
    "start": "parcel src/index.html --no-cache",
    "build": "parcel build src/index.html --out-dir ./",
    "tslint": "tslint --config tslint.json 'src/**/*.ts*'",
    "prettier": "prettier --write \"{components,src}/**/*.{ts,tsx,js,jsx,css,html}\"",
    "precommit": "yarn prettier && python cleanup.py && yarn build && git add src.* index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rafrex/spa-github-pages.git"
  },
  "author": "Amit Shah <amit@amwam.me>",
  "license": "MIT",
  "dependencies": {
    "@types/prismjs": "^1.9.0",
    "@types/react": "^16.7.3",
    "@types/react-dom": "^16.0.9",
    "@types/react-sidebar": "^3.0.0",
    "current-input": "^1.1.1",
    "git-pre-commit": "^2.1.4",
    "parcel": "^1.10.3",
    "prettier": "^1.15.2",
    "prismjs": "^1.15.0",
    "radium": "^0.25.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-markdown": "^4.0.3",
    "react-router": "^3.0.2",
    "react-sidebar": "^3.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "live-server": "^1.2.0",
    "redbox-react": "^1.5.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.1.6"
  }
}
